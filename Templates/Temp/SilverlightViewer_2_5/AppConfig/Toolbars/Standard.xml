<Toolbar StickyTools="{StickyTools}">
	<Tab DisplayName="Tools">
	<Group DisplayName="Home">
		<Button Name="HomePanel" DisplayName="Home" Command="ShowHomePanel" ImageUri="/Resources/Images/Icons/Toolbar/home-32.png" ToolTip="Show home panel" />
	</Group>
	<Group DisplayName="Navigation">
		<Button Name="Initial View" Command="ZoomToInitialExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-initial-32.png" ToolTip="Zoom to initial extent" />
		<Button Name="Back" Command="ZoomToPreviousExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-previous-32.png" ToolTip="Zoom to previous extent" />
		<Button Name="Forward" Command="ZoomToNextExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-next-32.png" ToolTip="Zoom to next extent" />
	</Group>
	<Group DisplayName="Find Data">
		<Tool Name="Identify" DisplayName="Identify" ToolTip="Identify features" DrawMode="Rectangle" Command="Identify" ImageUri="/Resources/Images/Icons/Toolbar/identify-32.png" HelpText="Click or drag on the map to identify features at that point." />
	</Group>
	<Group DisplayName="Tasks">
		<Button Name="Print" Command="PrintMap" ImageUri="/Resources/Images/Icons/Toolbar/print-32.png" ToolTip="Print a map" />
		<Button Name="Export" Command="ShowExportMapDialog" ImageUri="/Resources/Images/Icons/Toolbar/share-map-32.png" ToolTip="Export a map image" />
	</Group>
	{Insert__Toolbar-Tools-Groups}
	</Tab>
	{Insert__Toolbar-Tabs}
</Toolbar>