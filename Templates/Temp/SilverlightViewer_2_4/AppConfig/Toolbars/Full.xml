<Toolbar StickyTools="{StickyTools}">
  <Tab DisplayName="Home">
    {Insert__Toolbar-Home}
    <Group DisplayName="Home">
      <Button Name="HomePanel" DisplayName="Home" Command="ShowHomePanel" ImageUri="/Resources/Images/Icons/Toolbar/home-32.png" ToolTip="Show home panel" />
    </Group>
    <Group DisplayName="Navigation">
      <Button Name="Initial View" Command="ZoomToInitialExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-initial-32.png" ToolTip="Zoom to initial extent" />
      <Button Name="Full Extent" Command="ZoomToFullExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-full-32.png" ToolTip="Zoom to full extent" />
      <Button Name="Back" Command="ZoomToPreviousExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-previous-32.png" ToolTip="Zoom to previous extent" />
      <Button Name="Forward" Command="ZoomToNextExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-next-32.png" ToolTip="Zoom to next extent" />
    </Group>
    <Group DisplayName="Find Data">
      <Tool Name="Identify" DisplayName="Identify" ToolTip="Identify features" DrawMode="Rectangle" Command="Identify" ImageUri="/Resources/Images/Icons/Toolbar/identify-32.png" HelpText="Click or drag on the map to identify features at that point." />
    </Group>
    <Group DisplayName="Scale &amp; Extent">
      <Region RegionName="AdvancedControlsRegion" />
    </Group>
    <Group DisplayName="Tasks">
      <Button Name="Print" Command="PrintMap" ImageUri="/Resources/Images/Icons/Toolbar/print-32.png" ToolTip="Print a map" />
      <Button Name="Export" Command="ShowExportMapDialog" ImageUri="/Resources/Images/Icons/Toolbar/share-map-32.png" ToolTip="Export a map image" />
      <Button Name="Reports" Module="Reporting" Command="RunReport" ImageUri="/Resources/Images/Icons/Toolbar/reports-32.png" ToolTip="Create a report"/>
    </Group>
    {Insert__Toolbar-Home-Groups}
  </Tab>
  <Tab DisplayName="Data Sources">
    <Group DisplayName="Layers">
      <Button Name="Map Layers" Command="SwitchToLayerView" ImageUri="/Resources/Images/Icons/Toolbar/layers-32.png" ToolTip="Show layer list" />
      <Button Name="Add" Command="AddMapLayerInteractive" ImageUri="/Resources/Images/Icons/Toolbar/layers-add-32.png" ToolTip="Add external map layer" />
      <Button Name="Re-Order" Command="ShowLayerDrawingOrder" ImageUri="/Resources/Images/Icons/Toolbar/layers-reorder-32.png" ToolTip="Change the drawing order of layers" />
    </Group>
    <Group DisplayName="Import Layer">
      <Button Name="Shapefile" Command="OpenShapefileDialog" ImageUri="/Resources/Images/Icons/Toolbar/shapefile-add-32.png" ToolTip="Open a shapefile and add it to the map" />
      <Button Name="CSV File" Command="OpenCsvDialog" ImageUri="/Resources/Images/Icons/Toolbar/csv-add-32.png" ToolTip="Open a CSV file and add it to the map" />
      <Button Name="Address File" Command="OpenExcelFileDialog" ImageUri="/Resources/Images/Icons/Toolbar/geocode-batch-32.png" ToolTip="Open an Excel file and add it to the map" />
    </Group>
    <Group DisplayName="External Maps">
      <Button DisplayName="Google Maps™" Command="OpenWebPage" CommandParameter="../WMI/GoogleMaps.html" ImageUri="/Resources/Images/Icons/Toolbar/map-google-32.png" ToolTip="View the current location in Google Maps / Street View" />
      <Button DisplayName="Bing™ Maps" Command="OpenWebPage" CommandParameter="../WMI/BingMaps.html" ImageUri="/Resources/Images/Icons/Toolbar/map-bing-32.png" ToolTip="View the current location in Bing Maps / Birdseye" />
      {Insert__Toolbar-MapsAndDataSources-ExternalMapping-Items}
    </Group>
    {Insert__Toolbar-DataSources-Groups}
  </Tab>
  <Tab DisplayName="Find">
    <Group DisplayName="Find Data">
      <Region RegionName="IdentifyRegion" />
    </Group>
    <Group DisplayName="Map Tips">
      <Region RegionName="MapTipsRegion" />
    </Group>
    <Group DisplayName="Refine Data">
      <Button Name="Filter" Command="ActivateView" CommandParameter="FilterBuilderView" ImageUri="/Resources/Images/Icons/Toolbar/filter-32.png" ToolTip="Show advanced filter builder" />
      <Button Name="Query" Command="ActivateView" CommandParameter="SimpleQueryBuilderView" ImageUri="/Resources/Images/Icons/Toolbar/query-builder-32.png" ToolTip="Show simple query builder" />
      <Button Name="Advanced Query" Command="ActivateView" CommandParameter="QueryBuilderView" ImageUri="/Resources/Images/Icons/Toolbar/query-builder-advanced-32.png" ToolTip="Show advanced query builder" />
    </Group>
    <Group DisplayName="View">
      <Button Name="Results" Command="SwitchToResultsView" ImageUri="/Resources/Images/Icons/Toolbar/search-results-32.png" ToolTip="View current search results" />
      <Button Name="Selected" Command="SwitchToSelectionView" ImageUri="/Resources/Images/Icons/Toolbar/star-32.png" ToolTip="View selected features" />
    </Group>
    {Insert__Toolbar-Find-Groups}
  </Tab>
  <Tab DisplayName="Drawing">
    <Region RegionName="DrawingToolsRegion" />
    <Group DisplayName="Save">
      <Button Name="Save as Shapefile" Command="ExtractMarkup" ImageUri="/Resources/Images/Icons/Toolbar/draw-extract-32.png" ToolTip="Extract Markup" />
    </Group>
    <Group DisplayName="Settings">
      <Region RegionName="MarkupSnappingRegion" />
    </Group>
    {Insert__Toolbar-Drawing-Groups}
  </Tab>
  <Tab DisplayName="Analysis">
    <Group DisplayName="Measurement">
      <Region RegionName="MeasurementToolsRegion" />
    </Group>
    <Group DisplayName="Coordinates">
      <Region RegionName="CoordinateToolsRegion" />
    </Group>
    {Insert__Toolbar-Analysis-Groups}
  </Tab>
  <Tab DisplayName="Editing">
    <Region RegionName="EditToolsRegion" />
  </Tab>
  {Insert__Toolbar-Tabs}
</Toolbar>